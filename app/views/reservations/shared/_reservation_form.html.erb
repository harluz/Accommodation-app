<div class="field">
    <%= obj.label :check_in %><br />
    <%= obj.date_field :check_in, autofocus: true, autocomplete: "check_in" %>
  </div>

  <div class="field">
    <%= obj.label :check_out %><br />
    <%= obj.date_field :check_out, autofocus: true, autocomplete: "check_out" %>
  </div>

  <div class="field">
    <%= obj.label :count_person %>
    <%= obj.number_field :count_person, min: 0, autocomplete: "count_person" %>
  </div>